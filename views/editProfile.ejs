<div class="container" style="margin-top:40px">
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <strong>Edit Profile</strong>
        </div>
        <form action="/editProfile" method="POST">
          <fieldset>
            <div class="row">
                <div class="col-sm-12 col-md-10  col-md-offset-1 ">
                <br>
                <img class="center-block" src="https://thebenclark.files.wordpress.com/2014/03/facebook-default-no-profile-pic.jpg" width="150"></img>
                <br>
                <br>
                  <div class="form-group">
                    <div class="input-group">
                      <span class="input-group-addon">
                        <i class="glyphicon glyphicon-picture"></i>
                      </span>
                      <input class="form-control" name="photo" id="photo" type="text" placeholder="Enter picture link (optional)" autofocus>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="input-group">
                      <span class="input-group-addon">
                        <i class="glyphicon glyphicon-user"></i>
                      </span>
                      <input class="form-control" name="name" id="name" type="text" placeholder="Enter new name (optional)" autofocus>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="input-group">
                      <span class="input-group-addon">
                        <i class="glyphicon glyphicon-envelope"></i>
                      </span>
                      <input class="form-control"  name="email" id="email" type="email" placeholder="Enter new email (optional)">
                    </div>
                  </div>
                  <div class="form-group">
                    <input type="submit" class="btn btn-primary btn-block" value="Edit Profile Info">
                  </div>
                </div>
              </div>
          </fieldset>
        </form>
        <form action="/" method="GET">
          <button class="btn btn-primary btn-block">Back to Home</button>
        </form>

